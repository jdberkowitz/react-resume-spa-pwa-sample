{
  "name": "simple-react-full-stack",
  "version": "1.0.2",
  "description": "Sample resume app using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "index": "index.html",
  "scripts": {
    "start": "node src/server/index.js",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "webpack --mode production"
  },
  "author": "Joshua Berkowitz",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "@sendgrid/mail": "^6.3.1",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "npm": "^6.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.3.1",
    "react-service-worker": "^0.1.1",
    "react-social-media-icons": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
