    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "nodemon": "^1.17.3",
    "offline-plugin": "^5.0.5",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.23.0",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
